	.tabs {background-color:#eceef0;height:61px;border-bottom:1px solid #b8bcc0;width:100%;margin:0 auto;}
	.t_detail {width:1100px;margin:0 auto;}
	.t_container {position:relative; width:1100px;height:100%;margin:0 auto;}
	.t_container h1 {float:left; margin-top:27px}
	.tab_links {position:absolute;bottom:-1px;left:0px;width:100%;}
	.tab_links ul {display:inline}
	.tab_links ul li {display:inline; text-align:center;}
	.tab_links ul li a {
		text-decoration:none;		
		border:1px solid #b8bcc0;
		float:left;
		width:110px;
		border-bottom:1px solid #b8bcc0;
		background-color:#dce0e0;	
		margin-right:5px;
		padding:11px 10px;
		font-weight:bold;
		box-shadow: inset 0px 1px 0px #fff;
		-moz-border-radius:4px 4px 0px 0px;
		-webkit-border-radius:4px 4px 0px 0px;
		-khtml-border-radius:4px 4px 0px 0px;
		border-radius:4px 4px 0px 0px;
	}
	.tab_links li a:hover, .tab_links li a:active, .tab_links li a.active2 {
		border-bottom:1px solid #fff;
		background-color:#fff;			
	}
