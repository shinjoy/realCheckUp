@CHARSET "UTF-8";

/* Input Text */

/* Input Text */
INPUT[type=text].bb,INPUT[type=password].bb,
INPUT[type=date].bb,INPUT[type=time].bb
 {
	border:1px solid #b5b6b3;
	box-sizing:content-box;
	-ms-box-sizing:content-box;
	-moz-box-sizing:content-box;
	-webkit-box-sizing:content-box; 
	margin-top:-1px;
	padding:6px 6px;
	color:#8a8a8a;
	font-size:12px;
	height:15px;
	width:160px; 
}

LABEL {
	padding:6px 10px 0 0;
	vertical-align:-1px
}

INPUT[type=radio], INPUT[type=checkbox] {
	vertical-align:text-bottom;
	margin-right:1px;
}

/* Combo Box */
SELECT, SELECT:hover, SELECT:focus, SELECT:active {
	padding:5px 5px 5px 3px;
	margin-right:3px;
	border:1px solid #b5b6b3;
	-moz-border-radius:5px;
	-webkit-border-radius:5px;
	-khtml-border-radius:5px;
	border-radius:5px;
	color:#8a8a8a;
	font-size:12px;
	-moz-box-shadow: inset 0 0 10px #e9e9e9;
	-webkit-box-shadow: inset 0 0 10px #e9e9e9;
	box-shadow: inset  0 0 10px #e9e9e9;
	box-sizing:content-box;
	-ms-box-sizing:content-box;
	-moz-box-sizing:content-box;
	-webkit-box-sizing:content-box; 
}
SELECT.square, SELECT.square:hover, SELECT.square:focus, SELECT.square:active {
	padding:4px 4px 4px 4px;
	margin-right:3px;
	border:1px solid #b5b6b3;
	-moz-border-radius:0px;
	-webkit-border-radius:0px;
	-khtml-border-radius:0px;
	border-radius:0px;
	background:#eee;
	color:#8a8a8a;
	font-size:13px;
	font-family:"맑은 고딕"
	-moz-box-shadow: inset 0 0 10px #e9e9e9;
	-webkit-box-shadow: inset 0 0 10px #e9e9e9;
	box-shadow: inset  0 0 10px #e9e9e9;
	box-sizing:content-box;
	-ms-box-sizing:content-box;
	-moz-box-sizing:content-box;
	-webkit-box-sizing:content-box; 
}

/* TextArea */
textarea{
	border:1px solid #b5b6b3;
	-moz-border-radius:3px;
	-webkit-border-radius:3px;
	-khtml-border-radius:3px;
	border-radius:3px;
	color:#8a8a8a;
	font-size:12px;
	-moz-box-shadow: inset 0 0 10px #e9e9e9;
	-webkit-box-shadow: inset 0 0 10px #e9e9e9;
	box-shadow: inset  0 0 10px #e9e9e9;
	box-sizing:content-box;
	-ms-box-sizing:content-box;
	-moz-box-sizing:content-box;
	-webkit-box-sizing:content-box; 
	margin-right:4px;
	padding:6px 6px;
	height:13px;
	overflow:auto;
	vertical-align: top; 
	resize: vertical;
}

textarea:focus{
	border:1px solid #b5b6b3;
}

/* Button */
INPUT[type="submit"].bb, 
INPUT[type="button"].bb,
BUTTON.bb {
	display:inline-block;
	border:0px;
	letter-spacing:-0.5px;
	font-family:'돋움';
	font-size:12px;
	text-transform:uppercase;
	cursor:pointer;
	padding:6px 7px;
	height:27px;
}
A.button {
	display:inline-block;
	border:0px;
	letter-spacing:-0.5px;
	font-family:'돋움';
	font-size:12px;
	cursor:pointer;
	padding:6px 7px;
	color:#555;
}

INPUT[type="submit"].bb:hover, INPUT[type="submit"].bb:focus, INPUT[type="submit"].bb:active,
INPUT[type="button"].bb:hover, INPUT[type="button"].bb:focus, INPUT[type="button"].bb:active,
BUTTON.bb:hover, BUTTON.bb:focus, BUTTON.bb:active,
A.button:hover, A.button:focus, A.button:active {
	-moz-opacity: 0.8;
	 -khtml-opacity: 0.8;
	 opacity: 0.8;
	 filter:progid:DXImageTransform.Microsoft.Alpha(opacity=80);
	 text-decoration:none;
}

INPUT[type="submit"].disable:hover, INPUT[type="submit"].disable:focus, INPUT[type="submit"].disable:active,
INPUT[type="button"].disable:hover, INPUT[type="button"].disable:focus, INPUT[type="button"].disable:active,
BUTTON.disable:hover, BUTTON.disable:focus, BUTTON.disable:active,
A.disable:hover, A.disable:focus, A.disable:active {
	-moz-opacity: 1;
	 -khtml-opacity: 1;
	 opacity: 1;
	 filter:progid:DXImageTransform.Microsoft.Alpha(opacity=100);
	 text-decoration:none;
	 cursor:default;
}

INPUT[type="submit"].none, 
INPUT[type="button"].none,
BUTTON.none {
	display:inline-block;
	border:0px;
	letter-spacing:-0.5px;
	font-family:'돋움';
	font-size:12px;
	cursor:pointer;
	background:#fff;
}

INPUT[type="submit"].round, 
INPUT[type="button"].round,
BUTTON.round,
A.round {
	-moz-border-radius:3px;
	-webkit-border-radius:3px;
	-khtml-border-radius:3px;		
	border-radius:3px;
}

INPUT[type="submit"].big, 
INPUT[type="button"].big,
BUTTON.big {
	font-weight:bold;
	font-size:13px;
	padding:5px 15px;
	height:30px;
}
A.big {
	font-size:13px;
	padding:7px;
	height:30px;
}

INPUT[type="submit"].tiny, 
INPUT[type="button"].tiny,
BUTTON.tiny,
A.tiny {
	padding:2px 10px;
	height:17px;
}

INPUT[type="submit"].orange, 
INPUT[type="button"].orange,
BUTTON.orange,
A.orange {
	background-color:#F55A3F;
	color:#fff;
}

INPUT[type="submit"].green,
INPUT[type="button"].green,
BUTTON.green,
A.green {
	background-color:#a6d766;
	color:#fff;
}

INPUT[type="submit"].blue,
INPUT[type="button"].blue,
BUTTON.blue,
A.blue {
	background-color:#3dd4e1;
	color:#fff;
}

INPUT[type="submit"].gray, 
INPUT[type="button"].gray,
BUTTON.gray,
A.gray {
	color:#5c5c5c;
	border:1px solid #b5b6b3;
	background-color:#e7eaeb;
	text-shadow: 1px 1px 0px #fff;
}

INPUT[type="submit"].white, 
INPUT[type="button"].white,
BUTTON.white,
A.white {
	color:#5c5c5c;
	border:1px solid #b5b6b3;
	background-color:#fff;
}

INPUT[type="submit"].disable, 
INPUT[type="button"].disable,
BUTTON.disable,
A.disable {
	border:1px solid #eee;
	background-color:#f7f8f9;
	cursor:normal;
	color:#ccc;
}

/* File Input */
.file_input_textbox {
	float: left;
	width:260px; 
	padding:6px 6px;
	border:1px solid #b5b6b3;
	-moz-border-radius:5px;
	-webkit-border-radius:5px;
	-khtml-border-radius:5px;
	border-radius:5px;
	color:#8a8a8a;
	font-size:12px;
	-moz-box-shadow: inset 0 0 10px #e9e9e9;
	-webkit-box-shadow: inset 0 0 10px #e9e9e9;
	box-shadow: inset  0 0 10px #e9e9e9;
	box-sizing:content-box;
	-ms-box-sizing:content-box;
	-moz-box-sizing:content-box;
	-webkit-box-sizing:content-box; 
	margin-right:4px;
	margin-top:-1px;
	height:15px;
}
 
.file_input_div {
	position: relative;
	width: 80px;
	height: 30px;
	margin-left:0px;
	overflow: hidden;
}
.file_input_hidden {
	position: absolute;
	right: 0px;
	top: 0px;
	opacity: 0;
	filter: alpha(opacity=0);
	-ms-filter: "alpha(opacity=0)";
	-khtml-opacity: 0;
	-moz-opacity: 0;
}
.file_input_button {
	width: 80px;
	position: absolute;
	top: 0px;
	background-color: #33BB00;
	color: #FFFFFF;
	border-style: solid;
}

/* pagelist */
.pagelist {
	clear:left;
	margin:20px;
	text-align:center;
}
.pagelist button {
	margin:0;
	padding:0 6px;
	height:32px;
	cursor:pointer;
	vertical-align:top;
}
.pagelist button:hover {
	background:#fff;
	border:1px solid #c4c7d0;
}
.pagelist button.pageBtn {
	font-size:13px;
	color:#aaa;
	border:1px solid #f0f2f3;
}
.pagelist button.pageBtnCurrent {
	font-size:13px;
	color:#77f;
	border:1px solid #f0f2f3;
}
.pagelist button.boxBtn {
	background:url("/images/btn_boar.png") repeat-x;
	border:1px solid #c4c7d0;
	width:50px;
}
.pagelist button.boxBtnOff {
	background:url("/images/btn_boar.png") repeat-x;
	border:1px solid #c4c7d0;
	width:50px;
	cursor:default;
}
